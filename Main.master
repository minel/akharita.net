                    </ul>
                </div>
            </div>
        </div>

        <asp:ContentPlaceHolder ID="CphMidContents" runat="server">
        </asp:ContentPlaceHolder>

        <div class="footer">
            <div class="footer-item">
                <div class="item-pic">
                    <img src="/images/visit.png" />
